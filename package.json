{
  "name": "frontend-boilerplate",
  "version": "1.0.0",
  "private": true,
  "description": "A boilerplate of things that shouldn't exist",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors --port 3000",
    "build": "NODE_ENV=production webpack --progress --colors",
    "mocha": "NODE_ENV=test mocha --compilers js:babel-register --require ignore-styles -r should -r isomorphic-fetch -R spec client/**/__test__/*.spec.js",
    "istanbul": "NODE_ENV=test babel-node node_modules/istanbul/lib/cli cover --dir dist _mocha -- --compilers js:babel-register --require ignore-styles -r should -r isomorphic-fetch -R spec client/**/*.spec.js",
    "test": "NODE_ENV=test eslint ./ && npm run mocha"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.5.0",
    "classnames": "^2.2.3",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^4.0.0",
    "enzyme": "^2.1.0",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.1",
    "estraverse-fb": "^1.3.1",
    "fetch-mock": "^4.1.1",
    "file-loader": "^0.8.5",
    "ignore-styles": "^1.2.0",
    "istanbul": "1.0.0-alpha.2",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "postcss-loader": "^0.8.1",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "rucksack-css": "^0.8.5",
    "should": "^8.2.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.7.1"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1"
  }
}
